def add(a, b):
    return a + b
def subtract(a, b):
    return a - b
def multiply(a, b):
    return a * b
def divide(a, b):
    if b == 0:
        raise ValueError("Cannot divide by zero")
    return a / b

import mymath

a = 10
b = 5
print("Addition:", mymath.add(a, b))
print("Subtraction:", mymath.subtract(a, b))
print("Multiplication:", mymath.multiply(a, b))
print("Division:", mymath.divide(a, b))
